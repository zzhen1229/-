        <mxCell id="_Ws0ghuLOrh_OVOcMQFG-1" value="国富论" style="rounded=0;whiteSpace=wrap;html=1;" vertex="1" parent="1">
          <mxGeometry x="66" y="370" width="120" height="60" as="geometry" />
        </mxCell>
        <mxCell id="_Ws0ghuLOrh_OVOcMQFG-2" value="现代经济学发展史" style="rounded=0;whiteSpace=wrap;html=1;" vertex="1" parent="1">
          <mxGeometry x="214" y="200" width="120" height="60" as="geometry" />
        </mxCell>
        <mxCell id="_Ws0ghuLOrh_OVOcMQFG-3" value="15-17世纪初，英国海外贸易的发展和原始的基本积累" style="text;html=1;align=center;verticalAlign=middle;resizable=0;points=[];autosize=1;" vertex="1" parent="1">
          <mxGeometry x="404" y="170" width="300" height="20" as="geometry" />
        </mxCell>
        <mxCell id="_Ws0ghuLOrh_OVOcMQFG-4" value="英国资本主义迅速发展，资本主义的发展促使资产阶级新贵族的形成" style="text;html=1;align=center;verticalAlign=middle;resizable=0;points=[];autosize=1;" vertex="1" parent="1">
          <mxGeometry x="490" y="210" width="380" height="20" as="geometry" />
        </mxCell>
        <mxCell id="_Ws0ghuLOrh_OVOcMQFG-5" value="17世纪，斯图亚特王朝厉行专制统治，经常触犯资产阶级利益" style="text;html=1;align=center;verticalAlign=middle;resizable=0;points=[];autosize=1;" vertex="1" parent="1">
          <mxGeometry x="400" y="250" width="340" height="20" as="geometry" />
        </mxCell>
        <mxCell id="_Ws0ghuLOrh_OVOcMQFG-6" value="宗教政策进一步激化了阶级矛盾" style="text;html=1;align=center;verticalAlign=middle;resizable=0;points=[];autosize=1;" vertex="1" parent="1">
          <mxGeometry x="495" y="280" width="180" height="20" as="geometry" />
        </mxCell>
        <mxCell id="_Ws0ghuLOrh_OVOcMQFG-7" value="1640年 英国爆发资产阶级革命" style="text;html=1;align=center;verticalAlign=middle;resizable=0;points=[];autosize=1;" vertex="1" parent="1">
          <mxGeometry x="395" y="310" width="180" height="20" as="geometry" />
        </mxCell>
        <mxCell id="_Ws0ghuLOrh_OVOcMQFG-8" value="新兴资产阶级推翻专制统治，英国国王查理一世被送上断头台。" style="text;html=1;align=center;verticalAlign=middle;resizable=0;points=[];autosize=1;" vertex="1" parent="1">
          <mxGeometry x="485" y="350" width="350" height="20" as="geometry" />
        </mxCell>
        <mxCell id="_Ws0ghuLOrh_OVOcMQFG-9" value="继而建立起英国资本主义制度" style="text;html=1;align=center;verticalAlign=middle;resizable=0;points=[];autosize=1;" vertex="1" parent="1">
          <mxGeometry x="490" y="390" width="170" height="20" as="geometry" />
        </mxCell>
        <mxCell id="_Ws0ghuLOrh_OVOcMQFG-10" value="1662年英国的威廉配第发表了《赋税论》开创现代经济学" style="text;html=1;align=center;verticalAlign=middle;resizable=0;points=[];autosize=1;" vertex="1" parent="1">
          <mxGeometry x="395" y="430" width="320" height="20" as="geometry" />
        </mxCell>
        <mxCell id="_Ws0ghuLOrh_OVOcMQFG-11" value="威廉配第被称为英国古典政治经济学之父，统计学创始人，最早的宏观经济学者" style="text;html=1;align=center;verticalAlign=middle;resizable=0;points=[];autosize=1;" vertex="1" parent="1">
          <mxGeometry x="440" y="460" width="440" height="20" as="geometry" />
        </mxCell>
        <mxCell id="_Ws0ghuLOrh_OVOcMQFG-12" value="也是第一个提出“劳动价值论”的思想家" style="text;html=1;align=center;verticalAlign=middle;resizable=0;points=[];autosize=1;" vertex="1" parent="1">
          <mxGeometry x="440" y="480" width="220" height="20" as="geometry" />
        </mxCell>
        <mxCell id="_Ws0ghuLOrh_OVOcMQFG-13" value="其观点影响着后来者亚当斯密、李嘉图、马克思等" style="text;html=1;align=center;verticalAlign=middle;resizable=0;points=[];autosize=1;" vertex="1" parent="1">
          <mxGeometry x="440" y="510" width="280" height="20" as="geometry" />
        </mxCell>
        <mxCell id="_Ws0ghuLOrh_OVOcMQFG-14" value="劳动价值论：即价值是一种凝结在商品中无差别的人类劳动（威廉配第版）" style="text;html=1;align=center;verticalAlign=middle;resizable=0;points=[];autosize=1;" vertex="1" parent="1">
          <mxGeometry x="539" y="550" width="410" height="20" as="geometry" />
        </mxCell>
        <mxCell id="_Ws0ghuLOrh_OVOcMQFG-15" value="他认为社会的财富来源为土地和劳动" style="text;html=1;align=center;verticalAlign=middle;resizable=0;points=[];autosize=1;" vertex="1" parent="1">
          <mxGeometry x="539" y="575" width="210" height="20" as="geometry" />
        </mxCell>
        <mxCell id="_Ws0ghuLOrh_OVOcMQFG-16" value="而课税的最终对象也只能是土地的地租及其派生收入" style="text;html=1;align=center;verticalAlign=middle;resizable=0;points=[];autosize=1;" vertex="1" parent="1">
          <mxGeometry x="539" y="610" width="290" height="20" as="geometry" />
        </mxCell>
        <mxCell id="_Ws0ghuLOrh_OVOcMQFG-17" value="他把地租看成是剩余劳动的产物，从而也是赋税的最终源泉" style="text;html=1;align=center;verticalAlign=middle;resizable=0;points=[];autosize=1;" vertex="1" parent="1">
          <mxGeometry x="535" y="640" width="330" height="20" as="geometry" />
        </mxCell>
        <mxCell id="_Ws0ghuLOrh_OVOcMQFG-18" value="所谓的“劳动是财富之父，土地是财富之母”" style="text;html=1;align=center;verticalAlign=middle;resizable=0;points=[];autosize=1;" vertex="1" parent="1">
          <mxGeometry x="540" y="670" width="240" height="20" as="geometry" />
        </mxCell>
        <mxCell id="_Ws0ghuLOrh_OVOcMQFG-19" value="1705年 法国的重农学派代表布阿吉尔贝尔发表《论财富、货币、租税的性质》批评重商主义" style="text;html=1;align=center;verticalAlign=middle;resizable=0;points=[];autosize=1;" vertex="1" parent="1">
          <mxGeometry x="404" y="710" width="500" height="20" as="geometry" />
        </mxCell>
        <mxCell id="_Ws0ghuLOrh_OVOcMQFG-20" value="这里简单说下重商主义，重商主义主要商业本位，产生并流行于15世纪至17世纪中叶的西欧" style="text;html=1;align=center;verticalAlign=middle;resizable=0;points=[];autosize=1;" vertex="1" parent="1">
          <mxGeometry x="469" y="740" width="500" height="20" as="geometry" />
        </mxCell>
        <mxCell id="_Ws0ghuLOrh_OVOcMQFG-21" value="他建立在这样一个信念上：一国的国力通过贸易的顺差，即出口额大于进口额 获取财富" style="text;html=1;align=center;verticalAlign=middle;resizable=0;points=[];autosize=1;" vertex="1" parent="1">
          <mxGeometry x="479" y="770" width="470" height="20" as="geometry" />
        </mxCell>
        <mxCell id="_Ws0ghuLOrh_OVOcMQFG-22" value="该理论认为 一国积累的金银越多，就越富强" style="text;html=1;align=center;verticalAlign=middle;resizable=0;points=[];autosize=1;" vertex="1" parent="1">
          <mxGeometry x="469" y="810" width="250" height="20" as="geometry" />
        </mxCell>
        <mxCell id="_Ws0ghuLOrh_OVOcMQFG-23" value="主张国家干预经济生活，禁止金银输出，增加金银输入" style="text;html=1;align=center;verticalAlign=middle;resizable=0;points=[];autosize=1;" vertex="1" parent="1">
          <mxGeometry x="479" y="850" width="300" height="20" as="geometry" />
        </mxCell>
        <mxCell id="_Ws0ghuLOrh_OVOcMQFG-24" value="重商主义者认为要得到这种财富 最好由政府管制农业商业制造业&lt;br&gt;发展对外贸易垄断 通过高关税率及其他贸易限制来保护国内市场" style="text;html=1;align=center;verticalAlign=middle;resizable=0;points=[];autosize=1;" vertex="1" parent="1">
          <mxGeometry x="477" y="890" width="350" height="30" as="geometry" />
        </mxCell>
        <mxCell id="_Ws0ghuLOrh_OVOcMQFG-131" value="并利用殖民地为母国的制造业提供原料和市场" style="text;html=1;align=center;verticalAlign=middle;resizable=0;points=[];autosize=1;" vertex="1" parent="1">
          <mxGeometry x="469" y="920" width="260" height="20" as="geometry" />
        </mxCell>
        <mxCell id="_Ws0ghuLOrh_OVOcMQFG-132" value="不阿吉尔贝尔 反对货币是唯一财富的重商主义观点" style="text;html=1;align=center;verticalAlign=middle;resizable=0;points=[];autosize=1;" vertex="1" parent="1">
          <mxGeometry x="469" y="940" width="280" height="20" as="geometry" />
        </mxCell>
        <mxCell id="_Ws0ghuLOrh_OVOcMQFG-133" value="与对外贸易是财富源泉的观点相反 主张农业才是创造财富的最重要源泉" style="text;html=1;align=center;verticalAlign=middle;resizable=0;points=[];autosize=1;" vertex="1" parent="1">
          <mxGeometry x="479" y="970" width="390" height="20" as="geometry" />
        </mxCell>
        <mxCell id="_Ws0ghuLOrh_OVOcMQFG-134" value="他认为法国的200对个行业组成的财富的链条，其中农业是基础，各个行业保持互相间一定比例" style="text;html=1;align=center;verticalAlign=middle;resizable=0;points=[];autosize=1;" vertex="1" parent="1">
          <mxGeometry x="469" y="1000" width="520" height="20" as="geometry" />
        </mxCell>
        <mxCell id="_Ws0ghuLOrh_OVOcMQFG-135" value="各种产品主要以小麦等农产品为依据按比例进行交换" style="text;html=1;align=center;verticalAlign=middle;resizable=0;points=[];autosize=1;" vertex="1" parent="1">
          <mxGeometry x="485" y="1030" width="290" height="20" as="geometry" />
        </mxCell>
        <mxCell id="_Ws0ghuLOrh_OVOcMQFG-136" value="如果农业遭到破坏 整个国民经济就将崩溃" style="text;html=1;align=center;verticalAlign=middle;resizable=0;points=[];autosize=1;" vertex="1" parent="1">
          <mxGeometry x="479" y="1050" width="230" height="20" as="geometry" />
        </mxCell>
        <mxCell id="_Ws0ghuLOrh_OVOcMQFG-137" value="重商主义追求货币 损坏了农业 造成法国经济的衰败" style="text;html=1;align=center;verticalAlign=middle;resizable=0;points=[];autosize=1;" vertex="1" parent="1">
          <mxGeometry x="480" y="1070" width="290" height="20" as="geometry" />
        </mxCell>
        <mxCell id="_Ws0ghuLOrh_OVOcMQFG-140" value="" style="shape=image;verticalLabelPosition=bottom;labelBackgroundColor=#ffffff;verticalAlign=top;aspect=fixed;imageAspect=0;image=data:image/png,;" vertex="1" parent="1">
          <mxGeometry x="469" y="1180" width="520" height="123" as="geometry" />
        </mxCell>
        <mxCell id="_Ws0ghuLOrh_OVOcMQFG-141" value="他强调人们只能按自然规律办事 法国重商主义人为干预经济&lt;br&gt;违反自然呢规律 必将给法国带来灾害" style="text;html=1;align=center;verticalAlign=middle;resizable=0;points=[];autosize=1;" vertex="1" parent="1">
          <mxGeometry x="477" y="1100" width="330" height="30" as="geometry" />
        </mxCell>
        <mxCell id="_Ws0ghuLOrh_OVOcMQFG-142" value="他主张" style="text;html=1;align=center;verticalAlign=middle;resizable=0;points=[];autosize=1;" vertex="1" parent="1">
          <mxGeometry x="414" y="1149" width="50" height="20" as="geometry" />
        </mxCell>
        <mxCell id="_Ws0ghuLOrh_OVOcMQFG-143" value="不阿吉尔贝尔 以发展法国经济与提高国民经济福利为出发点" style="text;html=1;align=center;verticalAlign=middle;resizable=0;points=[];autosize=1;" vertex="1" parent="1">
          <mxGeometry x="450" y="1340" width="330" height="20" as="geometry" />
        </mxCell>
        <mxCell id="_Ws0ghuLOrh_OVOcMQFG-144" value="按财富多寡 实行公平征税作为努力的目标&lt;br&gt;实现贫者少缴 富者多缴" style="text;html=1;align=center;verticalAlign=middle;resizable=0;points=[];autosize=1;" vertex="1" parent="1">
          <mxGeometry x="455" y="1380" width="230" height="30" as="geometry" />
        </mxCell>
        <mxCell id="_Ws0ghuLOrh_OVOcMQFG-145" value="他们的理论深刻的影响亚当斯密" style="text;html=1;align=center;verticalAlign=middle;resizable=0;points=[];autosize=1;" vertex="1" parent="1">
          <mxGeometry x="450" y="1420" width="180" height="20" as="geometry" />
        </mxCell>
        <mxCell id="_Ws0ghuLOrh_OVOcMQFG-146" value="1776年 英国自由主义学派开山鼻祖 亚当斯密发表《国富论》" style="text;html=1;align=center;verticalAlign=middle;resizable=0;points=[];autosize=1;" vertex="1" parent="1">
          <mxGeometry x="310" y="1450" width="340" height="20" as="geometry" />
        </mxCell>
        <mxCell id="_Ws0ghuLOrh_OVOcMQFG-147" value="《国富论》的写作正值工业革命的前夜" style="text;html=1;align=center;verticalAlign=middle;resizable=0;points=[];autosize=1;" vertex="1" parent="1">
          <mxGeometry x="365" y="1480" width="220" height="20" as="geometry" />
        </mxCell>
        <mxCell id="_Ws0ghuLOrh_OVOcMQFG-148" value="当时手动业仍是英国资本主义生产的主要形式" style="text;html=1;align=center;verticalAlign=middle;resizable=0;points=[];autosize=1;" vertex="1" parent="1">
          <mxGeometry x="370" y="1510" width="260" height="20" as="geometry" />
        </mxCell>
        <mxCell id="_Ws0ghuLOrh_OVOcMQFG-149" value="但机器生产代替手工技术的趋势越来越明显" style="text;html=1;align=center;verticalAlign=middle;resizable=0;points=[];autosize=1;" vertex="1" parent="1">
          <mxGeometry x="375" y="1540" width="240" height="20" as="geometry" />
        </mxCell>
        <mxCell id="_Ws0ghuLOrh_OVOcMQFG-150" value="国富论主要有五篇 以下为第一篇内容" style="text;html=1;align=center;verticalAlign=middle;resizable=0;points=[];autosize=1;" vertex="1" parent="1">
          <mxGeometry x="225" y="1600" width="210" height="20" as="geometry" />
        </mxCell>
        <mxCell id="_Ws0ghuLOrh_OVOcMQFG-151" value="第一：亚当斯密认为劳动生产力上最大的进步 源于分工" style="text;html=1;align=center;verticalAlign=middle;resizable=0;points=[];autosize=1;" vertex="1" parent="1">
          <mxGeometry x="210" y="1640" width="310" height="20" as="geometry" />
        </mxCell>
        <mxCell id="_Ws0ghuLOrh_OVOcMQFG-152" value="第二：分工并不是人类智慧产物 而是人性中的利己心态" style="text;html=1;align=center;verticalAlign=middle;resizable=0;points=[];autosize=1;" vertex="1" parent="1">
          <mxGeometry x="206" y="1680" width="310" height="20" as="geometry" />
        </mxCell>
        <mxCell id="_Ws0ghuLOrh_OVOcMQFG-153" value="斯密一句著名的话：我们获取的食物，并未来自屠夫、酿酒师和面包师的恩惠" style="text;html=1;align=center;verticalAlign=middle;resizable=0;points=[];autosize=1;" vertex="1" parent="1">
          <mxGeometry x="265" y="1710" width="420" height="20" as="geometry" />
        </mxCell>
        <mxCell id="_Ws0ghuLOrh_OVOcMQFG-154" value="而是来自他们的利己思想" style="text;html=1;align=center;verticalAlign=middle;resizable=0;points=[];autosize=1;" vertex="1" parent="1">
          <mxGeometry x="276" y="1744" width="150" height="20" as="geometry" />
        </mxCell>
        <mxCell id="_Ws0ghuLOrh_OVOcMQFG-155" value="第三：分工受市场范围的限制" style="text;html=1;align=center;verticalAlign=middle;resizable=0;points=[];autosize=1;" vertex="1" parent="1">
          <mxGeometry x="210" y="1800" width="170" height="20" as="geometry" />
        </mxCell>
        <mxCell id="_Ws0ghuLOrh_OVOcMQFG-156" value="市场越小，分工程度越低，市场越大，分工越精细" style="text;html=1;align=center;verticalAlign=middle;resizable=0;points=[];autosize=1;" vertex="1" parent="1">
          <mxGeometry x="384" y="1800" width="280" height="20" as="geometry" />
        </mxCell>
        <mxCell id="_Ws0ghuLOrh_OVOcMQFG-157" value="第四：分工形成之后，人们会将自己消费不了的剩余劳动产物 同别人的剩余产品进行交换 来满足自己的大部分欲望" style="text;html=1;align=center;verticalAlign=middle;resizable=0;points=[];autosize=1;" vertex="1" parent="1">
          <mxGeometry x="214" y="1860" width="620" height="20" as="geometry" />
        </mxCell>
        <mxCell id="_Ws0ghuLOrh_OVOcMQFG-158" value="这样每个人都要靠 交换生活 或者说所有人都成了商人，而整个社会渐渐称为所谓的商业社会" style="text;html=1;align=center;verticalAlign=middle;resizable=0;points=[];autosize=1;" vertex="1" parent="1">
          <mxGeometry x="260" y="1890" width="500" height="20" as="geometry" />
        </mxCell>
        <mxCell id="_Ws0ghuLOrh_OVOcMQFG-159" value="而交换需要媒介，因此产生了货币，货币的演化也从最先的牲畜逐步演变成当时的贵金属铸币" style="text;html=1;align=center;verticalAlign=middle;resizable=0;points=[];autosize=1;" vertex="1" parent="1">
          <mxGeometry x="260" y="1910" width="510" height="20" as="geometry" />
        </mxCell>
        <mxCell id="_Ws0ghuLOrh_OVOcMQFG-160" value="当然从我们现代人看 ，贵金属货币也逐步被抛弃，成为了支付宝、微信程序里的一串代码" style="text;html=1;align=center;verticalAlign=middle;resizable=0;points=[];autosize=1;" vertex="1" parent="1">
          <mxGeometry x="255" y="1940" width="490" height="20" as="geometry" />
        </mxCell>
        <mxCell id="_Ws0ghuLOrh_OVOcMQFG-161" value="第五：一个人的贫富，由他可支配的劳动多少而定" style="text;html=1;align=center;verticalAlign=middle;resizable=0;points=[];autosize=1;" vertex="1" parent="1">
          <mxGeometry x="205" y="1990" width="280" height="20" as="geometry" />
        </mxCell>
        <mxCell id="_Ws0ghuLOrh_OVOcMQFG-162" value="也就是说劳动才是衡量一切商品内在交换价值的真实尺度" style="text;html=1;align=center;verticalAlign=middle;resizable=0;points=[];autosize=1;" vertex="1" parent="1">
          <mxGeometry x="260" y="2020" width="320" height="20" as="geometry" />
        </mxCell>
        <mxCell id="_Ws0ghuLOrh_OVOcMQFG-163" value="货币只不过是上商品的名义价格而已" style="text;html=1;align=center;verticalAlign=middle;resizable=0;points=[];autosize=1;" vertex="1" parent="1">
          <mxGeometry x="267" y="2050" width="210" height="20" as="geometry" />
        </mxCell>
        <mxCell id="_Ws0ghuLOrh_OVOcMQFG-169" value="第六：在资本积累和土地私有制尚未形成之前，获取物品所需要的劳动量被当成交换的唯一标准" style="text;html=1;align=center;verticalAlign=middle;resizable=0;points=[];autosize=1;" vertex="1" parent="1">
          <mxGeometry x="209" y="2100" width="520" height="20" as="geometry" />
        </mxCell>
        <mxCell id="_Ws0ghuLOrh_OVOcMQFG-170" value="原始社会状态下，全部劳动所得都归劳动者自己所有" style="text;html=1;align=center;verticalAlign=middle;resizable=0;points=[];autosize=1;" vertex="1" parent="1">
          <mxGeometry x="255" y="2140" width="290" height="20" as="geometry" />
        </mxCell>
        <mxCell id="_Ws0ghuLOrh_OVOcMQFG-171" value="当私有制确立后，一些人手动积累起资本" style="text;html=1;align=center;verticalAlign=middle;resizable=0;points=[];autosize=1;" vertex="1" parent="1">
          <mxGeometry x="255" y="2170" width="230" height="20" as="geometry" />
        </mxCell>
        <mxCell id="_Ws0ghuLOrh_OVOcMQFG-172" value="通过支付工资的形式雇佣工人，以期通过售卖工人的劳动产物来获取收入" style="text;html=1;align=center;verticalAlign=middle;resizable=0;points=[];autosize=1;" vertex="1" parent="1">
          <mxGeometry x="250" y="2200" width="400" height="20" as="geometry" />
        </mxCell>
        <mxCell id="_Ws0ghuLOrh_OVOcMQFG-173" value="这些收入减去原材料陈本以及支付给劳动者的工资 所形成的剩余 就是资本家所得的纯利润" style="text;html=1;align=center;verticalAlign=middle;resizable=0;points=[];autosize=1;" vertex="1" parent="1">
          <mxGeometry x="239" y="2240" width="490" height="20" as="geometry" />
        </mxCell>
        <mxCell id="_Ws0ghuLOrh_OVOcMQFG-174" value="而当一个国家土地完全私有化以后&lt;br&gt;那么地主便会通过出租土地的形式不劳而获&lt;br&gt;这些地租也成为了商品成本的第三个组成部分" style="text;html=1;align=left;verticalAlign=middle;resizable=0;points=[];autosize=1;" vertex="1" parent="1">
          <mxGeometry x="251" y="2280" width="260" height="50" as="geometry" />
        </mxCell>
        <mxCell id="_Ws0ghuLOrh_OVOcMQFG-175" value="这样构成商品的价格有三个组成部分：工资、地租、利润" style="text;html=1;align=center;verticalAlign=middle;resizable=0;points=[];autosize=1;" vertex="1" parent="1">
          <mxGeometry x="281" y="2370" width="320" height="20" as="geometry" />
        </mxCell>
        <mxCell id="_Ws0ghuLOrh_OVOcMQFG-176" value="在这里要插播一下：亚当斯密 其实漏了一些东西" style="text;html=1;align=center;verticalAlign=middle;resizable=0;points=[];autosize=1;" vertex="1" parent="1">
          <mxGeometry x="310" y="2420" width="270" height="20" as="geometry" />
        </mxCell>
        <mxCell id="_Ws0ghuLOrh_OVOcMQFG-177" value="学过会记学的应该能看出" style="text;html=1;align=center;verticalAlign=middle;resizable=0;points=[];autosize=1;" vertex="1" parent="1">
          <mxGeometry x="314" y="2440" width="150" height="20" as="geometry" />
        </mxCell>
        <mxCell id="_Ws0ghuLOrh_OVOcMQFG-178" value="主营业务收入指的是：一段时间内出售的商品价格的总和" style="text;html=1;align=center;verticalAlign=middle;resizable=0;points=[];autosize=1;" vertex="1" parent="1">
          <mxGeometry x="314" y="2470" width="320" height="20" as="geometry" />
        </mxCell>
        <mxCell id="_Ws0ghuLOrh_OVOcMQFG-179" value="主营业务成本呢包含购买原材料的价格、工人以及管理人员工资包含在管理费用" style="text;html=1;align=center;verticalAlign=middle;resizable=0;points=[];autosize=1;" vertex="1" parent="1">
          <mxGeometry x="310" y="2500" width="440" height="20" as="geometry" />
        </mxCell>
        <mxCell id="_Ws0ghuLOrh_OVOcMQFG-180" value="销售人员工资包含在营业费用，而土地如果是租赁，则租赁计入管理费用" style="text;html=1;align=center;verticalAlign=middle;resizable=0;points=[];autosize=1;" vertex="1" parent="1">
          <mxGeometry x="309" y="2530" width="400" height="20" as="geometry" />
        </mxCell>
        <mxCell id="_Ws0ghuLOrh_OVOcMQFG-181" value="土地如果自由，则折旧摊销费计入管理费用，此外还有财务费用，也就是借款利息，厂房折旧、专利使用费用、广告费用水电、网费等" style="text;html=1;align=center;verticalAlign=middle;resizable=0;points=[];autosize=1;" vertex="1" parent="1">
          <mxGeometry x="310" y="2570" width="720" height="20" as="geometry" />
        </mxCell>
        <mxCell id="_Ws0ghuLOrh_OVOcMQFG-182" value="不过斯密在国富论上说道，在任何一个社会 商品价格最终都要被分解成这三个部分中的一个或多个" style="text;html=1;align=center;verticalAlign=middle;resizable=0;points=[];autosize=1;" vertex="1" parent="1">
          <mxGeometry x="309" y="2620" width="530" height="20" as="geometry" />
        </mxCell>
        <mxCell id="_Ws0ghuLOrh_OVOcMQFG-183" value="也就是说 原材料价格也可以分成地租、工资、利润无限分下去" style="text;html=1;align=center;verticalAlign=middle;resizable=0;points=[];autosize=1;" vertex="1" parent="1">
          <mxGeometry x="314" y="2660" width="340" height="20" as="geometry" />
        </mxCell>
        <mxCell id="_Ws0ghuLOrh_OVOcMQFG-184" value="同理 其他各类费用也可以如此分下去" style="text;html=1;align=center;verticalAlign=middle;resizable=0;points=[];autosize=1;" vertex="1" parent="1">
          <mxGeometry x="315" y="2700" width="210" height="20" as="geometry" />
        </mxCell>
        <mxCell id="_Ws0ghuLOrh_OVOcMQFG-185" value="第七：斯密这里描述了“自然率”的概念&lt;br&gt;" style="text;html=1;align=center;verticalAlign=middle;resizable=0;points=[];autosize=1;" vertex="1" parent="1">
          <mxGeometry x="225" y="2785" width="220" height="20" as="geometry" />
        </mxCell>
        <mxCell id="_Ws0ghuLOrh_OVOcMQFG-186" value="这些普通率一部分由社会的贫富、进度等一般情况决定" style="text;html=1;align=center;verticalAlign=middle;resizable=0;points=[];autosize=1;" vertex="1" parent="1">
          <mxGeometry x="309" y="2850" width="300" height="20" as="geometry" />
        </mxCell>
        <mxCell id="_Ws0ghuLOrh_OVOcMQFG-187" value="一部分由各领域的特殊性质决定" style="text;html=1;align=center;verticalAlign=middle;resizable=0;points=[];autosize=1;" vertex="1" parent="1">
          <mxGeometry x="310" y="2870" width="180" height="20" as="geometry" />
        </mxCell>
        <mxCell id="_Ws0ghuLOrh_OVOcMQFG-188" value="这种普通率称为当地在某一时期通行的工资自然率" style="text;html=1;align=center;verticalAlign=middle;resizable=0;points=[];autosize=1;" vertex="1" parent="1">
          <mxGeometry x="314" y="2913" width="280" height="20" as="geometry" />
        </mxCell>
        <mxCell id="_Ws0ghuLOrh_OVOcMQFG-189" value="简而言之工资或者利润都有一个自然价格" style="text;html=1;align=center;verticalAlign=middle;resizable=0;points=[];autosize=1;" vertex="1" parent="1">
          <mxGeometry x="315" y="2960" width="230" height="20" as="geometry" />
        </mxCell>
        <mxCell id="_Ws0ghuLOrh_OVOcMQFG-190" value="基于上述观点，斯密认为一种商品的价格" style="text;html=1;align=center;verticalAlign=middle;resizable=0;points=[];autosize=1;" vertex="1" parent="1">
          <mxGeometry x="309" y="3000" width="230" height="20" as="geometry" />
        </mxCell>
        <mxCell id="_Ws0ghuLOrh_OVOcMQFG-191" value="恰好和它在生产制造和运输过程中" style="text;html=1;align=center;verticalAlign=middle;resizable=0;points=[];autosize=1;" vertex="1" parent="1">
          <mxGeometry x="314" y="3030" width="200" height="20" as="geometry" />
        </mxCell>
        <mxCell id="_Ws0ghuLOrh_OVOcMQFG-192" value="按照自然率支付的地租、工资和利润相等" style="text;html=1;align=center;verticalAlign=middle;resizable=0;points=[];autosize=1;" vertex="1" parent="1">
          <mxGeometry x="315" y="3060" width="230" height="20" as="geometry" />
        </mxCell>
        <mxCell id="_Ws0ghuLOrh_OVOcMQFG-193" value="斯密把愿意支付商品自然价格的人 称为有效需求者" style="text;html=1;align=center;verticalAlign=middle;resizable=0;points=[];autosize=1;" vertex="1" parent="1">
          <mxGeometry x="309" y="3090" width="280" height="20" as="geometry" />
        </mxCell>
        <mxCell id="_Ws0ghuLOrh_OVOcMQFG-194" value="把他们的需求称为有效需求" style="text;html=1;align=center;verticalAlign=middle;resizable=0;points=[];autosize=1;" vertex="1" parent="1">
          <mxGeometry x="295" y="3130" width="160" height="20" as="geometry" />
        </mxCell>
        <mxCell id="_Ws0ghuLOrh_OVOcMQFG-195" value="虽然商品的市场价格都在不断向自然价格靠拢" style="text;html=1;align=center;verticalAlign=middle;resizable=0;points=[];autosize=1;" vertex="1" parent="1">
          <mxGeometry x="300" y="3170" width="260" height="20" as="geometry" />
        </mxCell>
        <mxCell id="_Ws0ghuLOrh_OVOcMQFG-196" value="但有时由于一些特殊的原因 商品的市场价格高于自然价格" style="text;html=1;align=center;verticalAlign=middle;resizable=0;points=[];autosize=1;" vertex="1" parent="1">
          <mxGeometry x="294" y="3210" width="320" height="20" as="geometry" />
        </mxCell>
        <mxCell id="_Ws0ghuLOrh_OVOcMQFG-197" value="出于利益最大化，商人们往往刻意隐瞒这种变化而获取巨大利润" style="text;html=1;align=center;verticalAlign=middle;resizable=0;points=[];autosize=1;" vertex="1" parent="1">
          <mxGeometry x="290" y="3260" width="350" height="20" as="geometry" />
        </mxCell>
        <mxCell id="_Ws0ghuLOrh_OVOcMQFG-198" value="第八：基于上述第六点的分析：斯密提出" style="text;html=1;align=center;verticalAlign=middle;resizable=0;points=[];autosize=1;" vertex="1" parent="1">
          <mxGeometry x="111" y="3340" width="230" height="20" as="geometry" />
        </mxCell>
        <mxCell id="_Ws0ghuLOrh_OVOcMQFG-199" value="在土地私有和资本积累尚未形成之前的原始社会状态下" style="text;html=1;align=center;verticalAlign=middle;resizable=0;points=[];autosize=1;" vertex="1" parent="1">
          <mxGeometry x="220" y="3370" width="300" height="20" as="geometry" />
        </mxCell>
        <mxCell id="_Ws0ghuLOrh_OVOcMQFG-200" value="全部的劳动生产物都归劳动者支配" style="text;html=1;align=center;verticalAlign=middle;resizable=0;points=[];autosize=1;" vertex="1" parent="1">
          <mxGeometry x="220" y="3400" width="200" height="20" as="geometry" />
        </mxCell>
        <mxCell id="_Ws0ghuLOrh_OVOcMQFG-201" value="既没有地主也没有资本家来分享他的劳动所得" style="text;html=1;align=center;verticalAlign=middle;resizable=0;points=[];autosize=1;" vertex="1" parent="1">
          <mxGeometry x="219" y="3430" width="260" height="20" as="geometry" />
        </mxCell>
        <mxCell id="_Ws0ghuLOrh_OVOcMQFG-202" value="而当土地私有和资本积累出现后，这种劳动者独享劳动成果的状态宣告结束" style="text;html=1;align=center;verticalAlign=middle;resizable=0;points=[];autosize=1;" vertex="1" parent="1">
          <mxGeometry x="215" y="3460" width="410" height="20" as="geometry" />
        </mxCell>
        <mxCell id="_Ws0ghuLOrh_OVOcMQFG-203" value="土地一旦私有化 地主就从劳动者在土地上生产出来的劳动成果分走一部分" style="text;html=1;align=center;verticalAlign=middle;resizable=0;points=[];autosize=1;" vertex="1" parent="1">
          <mxGeometry x="234" y="3500" width="400" height="20" as="geometry" />
        </mxCell>
        <mxCell id="_Ws0ghuLOrh_OVOcMQFG-204" value="同理在手动业和制造业，资本家因为需要劳动者全部工作完成以前预先支付原材料和工资" style="text;html=1;align=center;verticalAlign=middle;resizable=0;points=[];autosize=1;" vertex="1" parent="1">
          <mxGeometry x="214" y="3540" width="480" height="20" as="geometry" />
        </mxCell>
        <mxCell id="_Ws0ghuLOrh_OVOcMQFG-205" value="而资本家所分享的劳动所得就成为了利润" style="text;html=1;align=center;verticalAlign=middle;resizable=0;points=[];autosize=1;" vertex="1" parent="1">
          <mxGeometry x="225" y="3580" width="230" height="20" as="geometry" />
        </mxCell>
        <mxCell id="_Ws0ghuLOrh_OVOcMQFG-206" value="这里插播一句，这个点是马克思剩余价值论和斯密的不同之处" style="text;html=1;align=center;verticalAlign=middle;resizable=0;points=[];autosize=1;" vertex="1" parent="1">
          <mxGeometry x="80" y="3660" width="340" height="20" as="geometry" />
        </mxCell>
        <mxCell id="_Ws0ghuLOrh_OVOcMQFG-207" value="马克思认为亚当斯密所认为资本家理所因当想有的利润" style="text;html=1;align=center;verticalAlign=middle;resizable=0;points=[];autosize=1;" vertex="1" parent="1">
          <mxGeometry x="145" y="3710" width="300" height="20" as="geometry" />
        </mxCell>
        <mxCell id="_Ws0ghuLOrh_OVOcMQFG-208" value="只不过斯密并没有认为这个不合理需要政府干预" style="text;html=1;align=center;verticalAlign=middle;resizable=0;points=[];autosize=1;" vertex="1" parent="1">
          <mxGeometry x="156" y="3750" width="270" height="20" as="geometry" />
        </mxCell>
        <mxCell id="_Ws0ghuLOrh_OVOcMQFG-209" value="而马克思认为只有废除私有制才能解决资本家对劳动者的剥削" style="text;html=1;align=center;verticalAlign=middle;resizable=0;points=[];autosize=1;" vertex="1" parent="1">
          <mxGeometry x="130" y="3800" width="340" height="20" as="geometry" />
        </mxCell>
        <mxCell id="_Ws0ghuLOrh_OVOcMQFG-210" value="这一章节，斯密分析了这个客观存在的想象" style="text;html=1;align=center;verticalAlign=middle;resizable=0;points=[];autosize=1;" vertex="1" parent="1">
          <mxGeometry x="135" y="3840" width="240" height="20" as="geometry" />
        </mxCell>
        <mxCell id="_Ws0ghuLOrh_OVOcMQFG-211" value="并没有像马克思一样分析，到底地主和资本家享受这些劳动成果是否合理" style="text;html=1;align=center;verticalAlign=middle;resizable=0;points=[];autosize=1;" vertex="1" parent="1">
          <mxGeometry x="145" y="3860" width="400" height="20" as="geometry" />
        </mxCell>
        <mxCell id="_Ws0ghuLOrh_OVOcMQFG-212" value="不过斯密还是阐述了他对仅以工资作为收入的劳动者的同情" style="text;html=1;align=center;verticalAlign=middle;resizable=0;points=[];autosize=1;" vertex="1" parent="1">
          <mxGeometry x="139" y="3890" width="330" height="20" as="geometry" />
        </mxCell>
        <mxCell id="_Ws0ghuLOrh_OVOcMQFG-213" value="他写道：地主、农场主、制造业者和商人即使一个工人都不雇佣，通常也能靠已经积累的资本生活一到两年&lt;br&gt;而劳动者一旦事业，能维持生活一个星期的都不多见，一个月就更少，一年几乎没有" style="text;html=1;align=left;verticalAlign=middle;resizable=0;points=[];autosize=1;" vertex="1" parent="1">
          <mxGeometry x="144" y="3940" width="580" height="30" as="geometry" />
        </mxCell>
        <mxCell id="_Ws0ghuLOrh_OVOcMQFG-214" value="就长期的观察来看，雇主对劳动者的需求程度和劳动者对雇主的需求程度大致相同" style="text;html=1;align=center;verticalAlign=middle;resizable=0;points=[];autosize=1;" vertex="1" parent="1">
          <mxGeometry x="140" y="3990" width="450" height="20" as="geometry" />
        </mxCell>
        <mxCell id="_Ws0ghuLOrh_OVOcMQFG-215" value="只不过雇主的需求不像劳动者那样迫切" style="text;html=1;align=center;verticalAlign=middle;resizable=0;points=[];autosize=1;" vertex="1" parent="1">
          <mxGeometry x="140" y="4040" width="220" height="20" as="geometry" />
        </mxCell>
        <mxCell id="_Ws0ghuLOrh_OVOcMQFG-216" value="国富论第一篇" style="rounded=0;whiteSpace=wrap;html=1;" vertex="1" parent="1">
          <mxGeometry x="90" y="1570" width="120" height="60" as="geometry" />
        </mxCell>
        <mxCell id="_Ws0ghuLOrh_OVOcMQFG-218" value="他写到：各个劳动领域的工资以及各投资领域的利润都存在一个普通率" style="text;html=1;align=center;verticalAlign=middle;resizable=0;points=[];autosize=1;" vertex="1" parent="1">
          <mxGeometry x="314" y="2820" width="390" height="20" as="geometry" />
        </mxCell>
        <mxCell id="_Ws0ghuLOrh_OVOcMQFG-219" value="独立思考 实时求是" style="text;html=1;align=center;verticalAlign=middle;resizable=0;points=[];autosize=1;" vertex="1" parent="1">
          <mxGeometry x="124" y="4140" width="110" height="20" as="geometry" />
        </mxCell>
        <mxCell id="_Ws0ghuLOrh_OVOcMQFG-220" value="我们对很多古代思想保有崇高的敬意" style="text;html=1;align=center;verticalAlign=middle;resizable=0;points=[];autosize=1;" vertex="1" parent="1">
          <mxGeometry x="165" y="4170" width="210" height="20" as="geometry" />
        </mxCell>
        <mxCell id="_Ws0ghuLOrh_OVOcMQFG-221" value="他们改变了世界，但是我们只是在巨人的脚下俯视巨人" style="text;html=1;align=center;verticalAlign=middle;resizable=0;points=[];autosize=1;" vertex="1" parent="1">
          <mxGeometry x="169" y="4190" width="300" height="20" as="geometry" />
        </mxCell>
        <mxCell id="_Ws0ghuLOrh_OVOcMQFG-222" value="而不对它进行证伪，那思想就是死的" style="text;html=1;align=center;verticalAlign=middle;resizable=0;points=[];autosize=1;" vertex="1" parent="1">
          <mxGeometry x="170" y="4220" width="210" height="20" as="geometry" />
        </mxCell>
        <mxCell id="_Ws0ghuLOrh_OVOcMQFG-223" value="社会也会停止不前，甚至倒退" style="text;html=1;align=center;verticalAlign=middle;resizable=0;points=[];autosize=1;" vertex="1" parent="1">
          <mxGeometry x="180" y="4240" width="170" height="20" as="geometry" />
        </mxCell>
        <mxCell id="_Ws0ghuLOrh_OVOcMQFG-224" value="这也是我国罢黜百家独尊儒术 后 思想界一直没有大发展的原因" style="text;html=1;align=center;verticalAlign=middle;resizable=0;points=[];autosize=1;" vertex="1" parent="1">
          <mxGeometry x="161" y="4270" width="350" height="20" as="geometry" />
        </mxCell>
        <mxCell id="_Ws0ghuLOrh_OVOcMQFG-225" value="这一点在北宋变法派王安石和守旧派司马光" style="text;html=1;align=center;verticalAlign=middle;resizable=0;points=[];autosize=1;" vertex="1" parent="1">
          <mxGeometry x="170" y="4300" width="240" height="20" as="geometry" />
        </mxCell>
        <mxCell id="_Ws0ghuLOrh_OVOcMQFG-226" value="关于社会的变革争论中体现的淋漓尽致" style="text;html=1;align=center;verticalAlign=middle;resizable=0;points=[];autosize=1;" vertex="1" parent="1">
          <mxGeometry x="156" y="4330" width="220" height="20" as="geometry" />
        </mxCell>
        <mxCell id="_Ws0ghuLOrh_OVOcMQFG-227" value="尤其是司马光强加给王安石 欣然接受的“三不足理论”" style="text;html=1;align=center;verticalAlign=middle;resizable=0;points=[];autosize=1;" vertex="1" parent="1">
          <mxGeometry x="155" y="4370" width="290" height="20" as="geometry" />
        </mxCell>
        <mxCell id="_Ws0ghuLOrh_OVOcMQFG-228" value="“天变不足畏，祖宗不足法，人言不足恤”" style="text;html=1;align=center;verticalAlign=middle;resizable=0;points=[];autosize=1;" vertex="1" parent="1">
          <mxGeometry x="156" y="4410" width="230" height="20" as="geometry" />
        </mxCell>
        <mxCell id="_Ws0ghuLOrh_OVOcMQFG-229" value="在司马光眼里 孔夫子是圣人 怎么会出错呢" style="text;html=1;align=center;verticalAlign=middle;resizable=0;points=[];autosize=1;" vertex="1" parent="1">
          <mxGeometry x="180" y="4450" width="240" height="20" as="geometry" />
        </mxCell>
        <mxCell id="_Ws0ghuLOrh_OVOcMQFG-230" value="王安石对抗老祖宗就是大逆不道" style="text;html=1;align=center;verticalAlign=middle;resizable=0;points=[];autosize=1;" vertex="1" parent="1">
          <mxGeometry x="180" y="4470" width="180" height="20" as="geometry" />
        </mxCell>
        <mxCell id="_Ws0ghuLOrh_OVOcMQFG-231" value="42年后 金军南下，靖康之役，俘虏徽钦二宗，北宋亡" style="text;html=1;align=center;verticalAlign=middle;resizable=0;points=[];autosize=1;" vertex="1" parent="1">
          <mxGeometry x="150" y="4500" width="300" height="20" as="geometry" />
        </mxCell>
        <mxCell id="_Ws0ghuLOrh_OVOcMQFG-232" value="随着1640年英国爆发资产阶级革命，西方原先重商主义制度制约了国家发展" style="text;html=1;align=center;verticalAlign=middle;resizable=0;points=[];autosize=1;" vertex="1" parent="1">
          <mxGeometry x="110" y="4540" width="410" height="20" as="geometry" />
        </mxCell>
        <mxCell id="_Ws0ghuLOrh_OVOcMQFG-233" value="新兴的资产阶级需要全新的一套理念 来指导建立国家" style="text;html=1;align=center;verticalAlign=middle;resizable=0;points=[];autosize=1;" vertex="1" parent="1">
          <mxGeometry x="110" y="4570" width="290" height="20" as="geometry" />
        </mxCell>
        <mxCell id="_Ws0ghuLOrh_OVOcMQFG-234" value="1662年，威廉配第 发表了《赋税论》开创信贷经济经济学，提出劳动价值论，并视地租及其派生收入为赋税的最终源泉" style="text;html=1;align=center;verticalAlign=middle;resizable=0;points=[];autosize=1;" vertex="1" parent="1">
          <mxGeometry x="105" y="4590" width="650" height="20" as="geometry" />
        </mxCell>
        <mxCell id="_Ws0ghuLOrh_OVOcMQFG-235" value="其后在1705年，重农派代表不阿吉尔贝尔批评重商主义并提出，农业才能创造财富" style="text;html=1;align=center;verticalAlign=middle;resizable=0;points=[];autosize=1;" vertex="1" parent="1">
          <mxGeometry x="60" y="4610" width="450" height="20" as="geometry" />
        </mxCell>
        <mxCell id="_Ws0ghuLOrh_OVOcMQFG-236" value="1758年，另一位法国重农学派代表魁奈发表《经济表》，提出货币流通仅仅是资本流通的要素，是要为再生产过程服务，受生产制约" style="text;html=1;align=center;verticalAlign=middle;resizable=0;points=[];autosize=1;" vertex="1" parent="1">
          <mxGeometry x="50" y="4630" width="710" height="20" as="geometry" />
        </mxCell>
        <mxCell id="_Ws0ghuLOrh_OVOcMQFG-237" value="从而总结了重商主义政策把国家致富之路放在流通领域的教训，重新提出以生产领域作为富国的根基" style="text;html=1;align=center;verticalAlign=middle;resizable=0;points=[];autosize=1;" vertex="1" parent="1">
          <mxGeometry x="40" y="4660" width="540" height="20" as="geometry" />
        </mxCell>
        <mxCell id="_Ws0ghuLOrh_OVOcMQFG-238" value="《经济表》再次论证了地主阶级是不事生产、不劳而获的阶级，因此得出十分重要的结论：地主阶级终日游手好闲" style="text;html=1;align=center;verticalAlign=middle;resizable=0;points=[];autosize=1;" vertex="1" parent="1">
          <mxGeometry x="36" y="4690" width="620" height="20" as="geometry" />
        </mxCell>
        <mxCell id="_Ws0ghuLOrh_OVOcMQFG-239" value="却以地租的方式占有纯产品，那么他们就应该担负起国家的全部税收" style="text;html=1;align=center;verticalAlign=middle;resizable=0;points=[];autosize=1;" vertex="1" parent="1">
          <mxGeometry x="40" y="4720" width="380" height="20" as="geometry" />
        </mxCell>
        <mxCell id="_Ws0ghuLOrh_OVOcMQFG-240" value="劳动阶级就应该被免除纳税的义务，以便更好的维持再生产的过程" style="text;html=1;align=center;verticalAlign=middle;resizable=0;points=[];autosize=1;" vertex="1" parent="1">
          <mxGeometry x="46" y="4750" width="360" height="20" as="geometry" />
        </mxCell>
        <mxCell id="_Ws0ghuLOrh_OVOcMQFG-241" value="不仅如此，国家还应该取消对他们的干预政策 放弃工业法规" style="text;html=1;align=center;verticalAlign=middle;resizable=0;points=[];autosize=1;" vertex="1" parent="1">
          <mxGeometry x="45" y="4780" width="330" height="20" as="geometry" />
        </mxCell>
        <mxCell id="_Ws0ghuLOrh_OVOcMQFG-242" value="废除行会制度，倡导自由竞争和自由对外贸易" style="text;html=1;align=center;verticalAlign=middle;resizable=0;points=[];autosize=1;" vertex="1" parent="1">
          <mxGeometry x="46" y="4810" width="260" height="20" as="geometry" />
        </mxCell>
        <mxCell id="_Ws0ghuLOrh_OVOcMQFG-243" value="对于魁奈的部分观点，马克思是持批判态度的" style="text;html=1;align=center;verticalAlign=middle;resizable=0;points=[];autosize=1;" vertex="1" parent="1">
          <mxGeometry x="50" y="4840" width="260" height="20" as="geometry" />
        </mxCell>
        <mxCell id="_Ws0ghuLOrh_OVOcMQFG-244" value="马克思在《资本论》 中就指出，重农学派名为重农" style="text;html=1;align=center;verticalAlign=middle;resizable=0;points=[];autosize=1;" vertex="1" parent="1">
          <mxGeometry x="50" y="4870" width="280" height="20" as="geometry" />
        </mxCell>
        <mxCell id="_Ws0ghuLOrh_OVOcMQFG-245" value="魁奈学说明显的反应出早期资产阶级学说的反封建特点，他提出的不干预工商业的理论 就是经济自由的原则" style="text;html=1;align=center;verticalAlign=middle;resizable=0;points=[];autosize=1;" vertex="1" parent="1">
          <mxGeometry x="350" y="4870" width="580" height="20" as="geometry" />
        </mxCell>
        <mxCell id="_Ws0ghuLOrh_OVOcMQFG-246" value="这正是当时资产阶级在经济上的追求理想" style="text;html=1;align=center;verticalAlign=middle;resizable=0;points=[];autosize=1;" vertex="1" parent="1">
          <mxGeometry x="350" y="4900" width="230" height="20" as="geometry" />
        </mxCell>
        <mxCell id="_Ws0ghuLOrh_OVOcMQFG-247" value="1776年英国海关的亚当斯密的儿子亚当斯密，发表《国富论》，瓦特则用这把钥匙开启了工业革命的大门" style="text;html=1;align=center;verticalAlign=middle;resizable=0;points=[];autosize=1;" vertex="1" parent="1">
          <mxGeometry x="50" y="4950" width="570" height="20" as="geometry" />
        </mxCell>
        <mxCell id="_Ws0ghuLOrh_OVOcMQFG-248" value="亚当斯密则用一只“看不见的手”为工业革命缔造了一个新的秩序" style="text;html=1;align=center;verticalAlign=middle;resizable=0;points=[];autosize=1;" vertex="1" parent="1">
          <mxGeometry x="54" y="4980" width="350" height="20" as="geometry" />
        </mxCell>
        <mxCell id="_Ws0ghuLOrh_OVOcMQFG-249" value="作为炎黄子孙我们，对于西方社会一举崛起称霸世界两百多年的理论源泉 一窥究竟" style="text;html=1;align=center;verticalAlign=middle;resizable=0;points=[];autosize=1;" vertex="1" parent="1">
          <mxGeometry x="54" y="5030" width="450" height="20" as="geometry" />
        </mxCell>
        <mxCell id="_Ws0ghuLOrh_OVOcMQFG-250" value="人大教授 金灿荣 说 西方社会在近代独步天下" style="text;html=1;align=center;verticalAlign=middle;resizable=0;points=[];autosize=1;" vertex="1" parent="1">
          <mxGeometry x="46" y="5060" width="250" height="20" as="geometry" />
        </mxCell>
        <mxCell id="_Ws0ghuLOrh_OVOcMQFG-251" value="打败天下无敌手的原因，就是西方比我们东方提前进入工业革命，掌握化" style="text;html=1;align=center;verticalAlign=middle;resizable=0;points=[];autosize=1;" vertex="1" parent="1">
          <mxGeometry x="20" y="5090" width="400" height="20" as="geometry" />
        </mxCell>
        <mxCell id="_Ws0ghuLOrh_OVOcMQFG-252" value="工业打农业 降维打击 没商量" style="text;html=1;align=center;verticalAlign=middle;resizable=0;points=[];autosize=1;" vertex="1" parent="1">
          <mxGeometry x="85" y="5120" width="170" height="20" as="geometry" />
        </mxCell>
        <mxCell id="_Ws0ghuLOrh_OVOcMQFG-253" value="资本利润" style="text;html=1;align=center;verticalAlign=middle;resizable=0;points=[];autosize=1;" vertex="1" parent="1">
          <mxGeometry x="30" y="5160" width="60" height="20" as="geometry" />
        </mxCell>
        <mxCell id="_Ws0ghuLOrh_OVOcMQFG-254" value="斯密提出：和劳动工资一样，资本利润的增加或者减少都取决于社会财富的增减" style="text;html=1;align=center;verticalAlign=middle;resizable=0;points=[];autosize=1;" vertex="1" parent="1">
          <mxGeometry x="55" y="5190" width="440" height="20" as="geometry" />
        </mxCell>
        <mxCell id="_Ws0ghuLOrh_OVOcMQFG-255" value="资本的增加，一方面使得工资提高，另一方面使得利润减少" style="text;html=1;align=center;verticalAlign=middle;resizable=0;points=[];autosize=1;" vertex="1" parent="1">
          <mxGeometry x="54" y="5230" width="330" height="20" as="geometry" />
        </mxCell>
        <mxCell id="_Ws0ghuLOrh_OVOcMQFG-256" value="他举例：如果有许多富商投资同一行业，他们相互竞争自然会促使这一行业的利润减少" style="text;html=1;align=center;verticalAlign=middle;resizable=0;points=[];autosize=1;" vertex="1" parent="1">
          <mxGeometry x="69" y="5261" width="470" height="20" as="geometry" />
        </mxCell>
        <mxCell id="_Ws0ghuLOrh_OVOcMQFG-257" value="同一社会中 如果各种行业的资本都这样增加了，那么这种竞争必将使所有行业的利润趋于减少" style="text;html=1;align=center;verticalAlign=middle;resizable=0;points=[];autosize=1;" vertex="1" parent="1">
          <mxGeometry x="120" y="5290" width="510" height="20" as="geometry" />
        </mxCell>
        <mxCell id="_Ws0ghuLOrh_OVOcMQFG-258" value="斯密指出：即使对特定地方在特定时间的劳动平均工资也无法给出具体数字" style="text;html=1;align=center;verticalAlign=middle;resizable=0;points=[];autosize=1;" vertex="1" parent="1">
          <mxGeometry x="54" y="5320" width="410" height="20" as="geometry" />
        </mxCell>
        <mxCell id="_Ws0ghuLOrh_OVOcMQFG-259" value="同理要想知道一个行业的平均利润更是难上加难" style="text;html=1;align=center;verticalAlign=middle;resizable=0;points=[];autosize=1;" vertex="1" parent="1">
          <mxGeometry x="115" y="5340" width="270" height="20" as="geometry" />
        </mxCell>
        <mxCell id="_Ws0ghuLOrh_OVOcMQFG-260" value="以现代人的视角 我们经常能从国家统计局 得到平均工资或者行业平均利润&amp;nbsp; 得益于现在互联网发展&amp;nbsp;" style="text;html=1;align=center;verticalAlign=middle;resizable=0;points=[];autosize=1;" vertex="1" parent="1">
          <mxGeometry x="49" y="5380" width="540" height="20" as="geometry" />
        </mxCell>
        <mxCell id="_Ws0ghuLOrh_OVOcMQFG-261" value="斯密指出：虽然无法获取到平均工资 或者平均 利润 但是在货币的利息上能得出大概，收益越高的地方通常利息越高，反之依然" style="text;html=1;align=center;verticalAlign=middle;resizable=0;points=[];autosize=1;" vertex="1" parent="1">
          <mxGeometry x="50" y="5430" width="690" height="20" as="geometry" />
        </mxCell>
        <mxCell id="_Ws0ghuLOrh_OVOcMQFG-262" value="由此斯密断定，一个国家资本的一般利润，必然会随着市场一般利率的变动而变动" style="text;html=1;align=center;verticalAlign=middle;resizable=0;points=[];autosize=1;" vertex="1" parent="1">
          <mxGeometry x="55" y="5460" width="450" height="20" as="geometry" />
        </mxCell>
        <mxCell id="_Ws0ghuLOrh_OVOcMQFG-263" value="利率降低 利润也会降低；利率升高 利润自然会升高" style="text;html=1;align=center;verticalAlign=middle;resizable=0;points=[];autosize=1;" vertex="1" parent="1">
          <mxGeometry x="545" y="5460" width="290" height="20" as="geometry" />
        </mxCell>
        <mxCell id="_Ws0ghuLOrh_OVOcMQFG-264" value="随着国家财富的增长 劳动者的工资也在不断增加" style="text;html=1;align=center;verticalAlign=middle;resizable=0;points=[];autosize=1;" vertex="1" parent="1">
          <mxGeometry x="55" y="5490" width="270" height="20" as="geometry" />
        </mxCell>
        <mxCell id="_Ws0ghuLOrh_OVOcMQFG-265" value="而同时绝大多数 工商业的资本利润却在不断减少" style="text;html=1;align=center;verticalAlign=middle;resizable=0;points=[];autosize=1;" vertex="1" parent="1">
          <mxGeometry x="345" y="5490" width="270" height="20" as="geometry" />
        </mxCell>
        <mxCell id="_Ws0ghuLOrh_OVOcMQFG-266" value="反之贫穷的国家由于缺少充足资本来雇佣全部劳动力 为了获得职业" style="text;html=1;align=center;verticalAlign=middle;resizable=0;points=[];autosize=1;" vertex="1" parent="1">
          <mxGeometry x="50" y="5530" width="370" height="20" as="geometry" />
        </mxCell>
        <mxCell id="_Ws0ghuLOrh_OVOcMQFG-267" value="劳动者必然相互竞争 导致劳动工资降低 并使资本利润增加" style="text;html=1;align=center;verticalAlign=middle;resizable=0;points=[];autosize=1;" vertex="1" parent="1">
          <mxGeometry x="54" y="5560" width="320" height="20" as="geometry" />
        </mxCell>
        <mxCell id="_Ws0ghuLOrh_OVOcMQFG-268" value="由于客观条件限制 斯密得出结论 通常也是观察当时世界的运行 现象而得出 但是作为一门学科 并不严谨" style="text;html=1;align=center;verticalAlign=middle;resizable=0;points=[];autosize=1;" vertex="1" parent="1">
          <mxGeometry x="41" y="5590" width="560" height="20" as="geometry" />
        </mxCell>
        <mxCell id="_Ws0ghuLOrh_OVOcMQFG-269" value="科学需要进过反复论证 直到一定限度内找不到反例" style="text;html=1;align=center;verticalAlign=middle;resizable=0;points=[];autosize=1;" vertex="1" parent="1">
          <mxGeometry x="39" y="5620" width="280" height="20" as="geometry" />
        </mxCell>
        <mxCell id="_Ws0ghuLOrh_OVOcMQFG-270" value="而这个理论在这个限度内有效&amp;nbsp;" style="text;html=1;align=center;verticalAlign=middle;resizable=0;points=[];autosize=1;" vertex="1" parent="1">
          <mxGeometry x="280" y="5650" width="170" height="20" as="geometry" />
        </mxCell>
        <mxCell id="_Ws0ghuLOrh_OVOcMQFG-271" value="就像经典物理学中的牛顿万有引力" style="text;html=1;align=center;verticalAlign=middle;resizable=0;points=[];autosize=1;" vertex="1" parent="1">
          <mxGeometry x="34" y="5680" width="200" height="20" as="geometry" />
        </mxCell>
        <mxCell id="_Ws0ghuLOrh_OVOcMQFG-272" value="在低速条件下 经典物理基本可以用于客观现象解释 但是在物体接近光速的情况下 经典物理在世纪观察中就会出现偏差" style="text;html=1;align=center;verticalAlign=middle;resizable=0;points=[];autosize=1;" vertex="1" parent="1">
          <mxGeometry x="41" y="5700" width="640" height="20" as="geometry" />
        </mxCell>
        <mxCell id="_Ws0ghuLOrh_OVOcMQFG-273" value="而在微观层面，经典物理同样也无法解释客观规律 而不得不引入量子力学" style="text;html=1;align=center;verticalAlign=middle;resizable=0;points=[];autosize=1;" vertex="1" parent="1">
          <mxGeometry x="35" y="5720" width="400" height="20" as="geometry" />
        </mxCell>
        <mxCell id="_Ws0ghuLOrh_OVOcMQFG-274" value="斯密的论点" style="text;html=1;align=center;verticalAlign=middle;resizable=0;points=[];autosize=1;" vertex="1" parent="1">
          <mxGeometry x="20" y="5750" width="80" height="20" as="geometry" />
        </mxCell>
        <mxCell id="_Ws0ghuLOrh_OVOcMQFG-275" value="资本的增加使工资增加，同时使利润减少" style="text;html=1;align=center;verticalAlign=middle;resizable=0;points=[];autosize=1;" vertex="1" parent="1">
          <mxGeometry x="110" y="5750" width="230" height="20" as="geometry" />
        </mxCell>
        <mxCell id="_Ws0ghuLOrh_OVOcMQFG-276" value="通过观察现状可知：富士康建设新生产线，工人的工资也会随着资本的增加而增加吗？" style="text;html=1;align=center;verticalAlign=middle;resizable=0;points=[];autosize=1;" vertex="1" parent="1">
          <mxGeometry x="20" y="5770" width="470" height="20" as="geometry" />
        </mxCell>
        <mxCell id="_Ws0ghuLOrh_OVOcMQFG-277" value="在乔布斯的带领下，苹果公司将大量资本投资iphone4的研发" style="text;html=1;align=center;verticalAlign=middle;resizable=0;points=[];autosize=1;" vertex="1" parent="1">
          <mxGeometry x="114" y="5790" width="330" height="20" as="geometry" />
        </mxCell>
        <mxCell id="_Ws0ghuLOrh_OVOcMQFG-278" value="苹果的利润减少了吗" style="text;html=1;align=center;verticalAlign=middle;resizable=0;points=[];autosize=1;" vertex="1" parent="1">
          <mxGeometry x="440" y="5790" width="120" height="20" as="geometry" />
        </mxCell>
        <mxCell id="_Ws0ghuLOrh_OVOcMQFG-279" value="资本的增加与工资的增加以及利润的增加 与否之间没有必然的联系" style="text;html=1;align=center;verticalAlign=middle;resizable=0;points=[];autosize=1;" vertex="1" parent="1">
          <mxGeometry x="49" y="5840" width="370" height="20" as="geometry" />
        </mxCell>
        <mxCell id="_Ws0ghuLOrh_OVOcMQFG-280" value="微观层面如此，宏观层面上" style="text;html=1;align=center;verticalAlign=middle;resizable=0;points=[];autosize=1;" vertex="1" parent="1">
          <mxGeometry x="20" y="5880" width="160" height="20" as="geometry" />
        </mxCell>
        <mxCell id="_Ws0ghuLOrh_OVOcMQFG-281" value="美国的财富远高于墨西哥，那是否为美国的各个行业平均工资都高于墨西哥你呢" style="text;html=1;align=center;verticalAlign=middle;resizable=0;points=[];autosize=1;" vertex="1" parent="1">
          <mxGeometry x="214" y="5880" width="440" height="20" as="geometry" />
        </mxCell>
        <mxCell id="_Ws0ghuLOrh_OVOcMQFG-282" value="当然 主要斯密时期还没有“比较优势”的概念" style="text;html=1;align=center;verticalAlign=middle;resizable=0;points=[];autosize=1;" vertex="1" parent="1">
          <mxGeometry x="39" y="5930" width="240" height="20" as="geometry" />
        </mxCell>
        <mxCell id="_Ws0ghuLOrh_OVOcMQFG-283" value="比较优势理论：国际贸易的基础是生产技术的相对差别，以及由此产生的相对成本差别" style="text;html=1;align=center;verticalAlign=middle;resizable=0;points=[];autosize=1;" vertex="1" parent="1">
          <mxGeometry x="95" y="5960" width="470" height="20" as="geometry" />
        </mxCell>
        <mxCell id="_Ws0ghuLOrh_OVOcMQFG-284" value="英国古典经济学家 大卫.李嘉图" style="text;html=1;align=center;verticalAlign=middle;resizable=0;points=[];autosize=1;" vertex="1" parent="1">
          <mxGeometry x="105" y="5990" width="180" height="20" as="geometry" />
        </mxCell>
        <mxCell id="_Ws0ghuLOrh_OVOcMQFG-285" value="基于斯密的上诉观点，他提出随着财富、人口的增加和劳动的改良，利息下降了，但劳动工资却没有随着资本利润的减少而下降" style="text;html=1;align=center;verticalAlign=middle;resizable=0;points=[];autosize=1;" vertex="1" parent="1">
          <mxGeometry x="69" y="6020" width="690" height="20" as="geometry" />
        </mxCell>
        <mxCell id="_Ws0ghuLOrh_OVOcMQFG-286" value="无论利润如何，对劳动的需求都会随着资本的增加而增加" style="text;html=1;align=center;verticalAlign=middle;resizable=0;points=[];autosize=1;" vertex="1" parent="1">
          <mxGeometry x="90" y="6050" width="320" height="20" as="geometry" />
        </mxCell>
        <mxCell id="_Ws0ghuLOrh_OVOcMQFG-287" value="利润减少了，资本不仅继续增加 而且还比原来增加的更快" style="text;html=1;align=center;verticalAlign=middle;resizable=0;points=[];autosize=1;" vertex="1" parent="1">
          <mxGeometry x="56" y="6080" width="320" height="20" as="geometry" />
        </mxCell>
        <mxCell id="_Ws0ghuLOrh_OVOcMQFG-288" value="当时斯密是基于当时社会背景，当时大英帝国海外扩张殖民地时代" style="text;html=1;align=center;verticalAlign=middle;resizable=0;points=[];autosize=1;" vertex="1" parent="1">
          <mxGeometry x="-110" y="6130" width="360" height="20" as="geometry" />
        </mxCell>
      </root>
    </mxGraphModel>
  </diagram>
</mxfile>
